<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TimeLine">

    <com.akribase.timelineview.TimelineView
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/timeline_view"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:paddingTop="16dp"
        app:startLabel="9"
        app:backgroundColor="@color/green_background_color"
        app:eventBackground="@color/gray"
        app:labelColor="@color/light_green"
        app:eventNameColor="@color/white"
        app:eventTimeColor="@color/white" />

</androidx.constraintlayout.widget.ConstraintLayout>
